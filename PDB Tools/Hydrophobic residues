import os

with open ("F:/.pdb") as data:
	p = data.read().splitlines()
g= set(p)


for t in g: 
	k=t.split()
	if k[0] == 'ATOM':
		if k[3] == 'ILE':
			if k[3] == 'ALA':
				if k[3] == 'GLY': 
					if k[3] == 'PHE':
						if k[3] == 'MET':
						


with open("example.pdb", "w") as f:
	for u in t:
		f.writelines(u)
f.close()
